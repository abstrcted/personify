# Example .env file for Personify (do NOT commit real secrets)
# Copy this to `.env` and replace placeholders with real values.

# Spotify API Credentials (Backend - DO NOT expose in frontend!)
SPOTIFY_CLIENT_ID=YOUR_SPOTIFY_CLIENT_ID_HERE
SPOTIFY_CLIENT_SECRET=YOUR_SPOTIFY_CLIENT_SECRET_HERE
SPOTIFY_REDIRECT_URI=http://127.0.0.1:3001/callback

# Server Configuration
PORT=3001
FRONTEND_URI=http://127.0.0.1:5174

# GetSongBPM API
GETSONGBPM_API_KEY=YOUR_GETSONGBPM_API_KEY_HERE

# RAPIDAPI
RAPIDAPI_KEY=YOUR_RAPIDAPI_KEY_HERE
